							<td class="printleft">Пол</td>
							<td><& cl[gender_id][:name] &>&nbsp;</td>
							<td><& cl[attributes['gender_id']][:name] &>&nbsp;</td>
							<td><& attributes['gender_basics'] &>&nbsp;</td>
						</tr>
					<% end %>

					<% if card_number != attributes['card_number'] %>
