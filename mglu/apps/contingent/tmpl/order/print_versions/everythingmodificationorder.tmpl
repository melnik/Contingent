	student_id = _ITEM_[:student_id]
	name = _ITEM_[:name]
	gender_id = _ITEM_[:gender_id]
	card_number = _ITEM_[:card_number]
	group_id = _ITEM_[:group_id]
	birth_date = _ITEM_[:birth_date]
	citizenship_id = _ITEM_[:citizenship_id]
