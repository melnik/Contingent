							<td class="printleft">Дата рождения</td>
							<td><& birth_date &>&nbsp;</td>
							<td><& attributes['birth_date'] &>&nbsp;</td>
							<td><& attributes['birth_date_basics'] &>&nbsp;</td>
						</tr>
					<% end %>

					<% if study_type_id != attributes['study_type_id'] %>
						<% cl = Classifier::StudyType.new %>
