<!ATTR order !>
<!-- Generated by details-generator. Do not edit! -->
<%
_CONDITIONS_ = ::DetailsForCancellationOrder.order_conditions(order)
conditions = _CONDITIONS_
%>
<**header|set_header_attr**>
<tr><td> <% o = Order.load order.attributes['order_id'] %> </td></tr>
<tr><td align="center"><h3>ПРИКАЗЫВАЮ:</h3></td></tr>
</table>
<% print_version = :text %>

<% has_foreign = false %>


<%
i = 0
order.each_student(:all, %w( student_id attributes)) do |student_id, attributes|
	( s = Student.load student_id ).recondition o.date_activated
%>
<h3>&#167; <%=i += 1%></h3>
<p>
<div> Приказ по университету от <&o.date_activated&> &#x2116;<&o.number&> <&o.class::TYPE&>: </div>

<%=s.name.gsub(' ', '&nbsp;')%>;
<%=s.status.to_html%>;
Л.д. &#x2116;: <%=s.card_number.to_html%>;
Дата рождения: <%=s.birth_date.to_html%>;
Курс: <%=s.course.to_html%>;
Группа: <%=s.group.to_html%>;
<div>отменить в связи с вновь открывшимися обстоятельствами.
Числить в группе <%= Group.load(s.group_id).to_html %>.</div>

<% unless attributes['agreement'].empty? then %>Договор: <&attributes['agreement']&>;
<% end %>

<div>

				<% case o.class.to_s.intern
				when :ExpelationOrder then %>
					Декану факультета, при необходимости, установить  срок сдачи академических задолженностей своим распоряжением.
				<% end %>

			</div>

<div>
				Основания приказа:
				<% case attributes['basics']
				when 1 then %>
					личное заявление отчисленного с визой декана факультета и резолюцией ректора,
					документы, подтверждающие вновь открывшиеся обстоятельства.
				<% when 2 then %>
					личное заявление студента с визой декана факультета и резолюцией ректора.
				<% when 3 then %>
					служебная записка декана факультета с резолюцией ректора.
				<% when 4 then %>
					служебная записка декана ФОИГ с резолюцией ректора.
				<% when 5 then %>
					личное заявление отчисленного с визой декана факультета, декана ФОИГ и резолюцией ректора,
					документы, подтверждающие вновь открывшиеся обстоятельства.
				<% when 6 then %>
					личное заявление студента с визой декана факультета, декана ФОИГ
					и резолюцией ректора.
				<% when 7 then %>
					служебная записка декана факультета с визой декана ФОИГ и резолюцией ректора.
				<% end %>
				
				<% has_foreign ||= attributes['basics'].in? [4, 5, 6, 7] %>
			</div>
</p>

<% end %>
<br/>
<br/>
<br/>

<% _SIGNATURE_INDEX_ = 0 %>
